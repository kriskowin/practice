budget = float(input())
statists = int(input())
statists_cloth_price = float(input())
decor = 0.1 * budget

if statists > 150:
    statists_cloth_price *= 0.9

statists_cloth_price *= statists
total = budget - (statists_cloth_price + decor)

if total >= 0:
    print("Action!")
    print(f"Wingard starts filming with {total:.2f} leva left.")
else:
    print("Not enough money!")
    print(f"Wingard needs {abs(total):.2f} leva more.")
