data = input().split(", ")
rows = int(data[0])
cols = int(data[1])
matrix = []
total = 0

for _ in range(rows):
    data_rows = input().split(", ")
    row_data = []

    for i in range(len(data_rows)):
        current_element = int(data_rows[i])
        total += current_element
        row_data.append(int(data_rows[i]))
    matrix.append(row_data)

print(total)
print(matrix)
