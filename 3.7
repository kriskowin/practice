figure = input()
if figure == "square":
    side = float(input())
    print(f"{side * side:.3f}")

elif figure == "rectangle":
    side_a = float(input())
    side_b = float(input())
    print(f"{side_a * side_b:.3f}")

elif figure == "circle":
    radius = float(input())
    print(f"{3.14159 * radius * radius:.3f}")

elif figure == "triangle":
    side_a = float(input())
    side_b = float(input())
    print(f"{side_a * (side_b / 2):.3f}")
